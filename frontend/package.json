{
  "name": "observatory",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "@wojtekmaj/react-daterange-picker": "^3.3.2",
    "d3-celestial": "^0.7.35",
    "highcharts": "^9.3.0",
    "highcharts-react-official": "^3.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-image-lightbox": "^5.1.4",
    "react-photo-gallery": "^8.0.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^5.0.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "suncalc": "^1.8.0",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts build",
    "test": "react-scripts test src/__tests__",
    "test:coverage": "react-scripts test --coverage src/__tests__",
    "eject": "react-scripts eject",
    "mswinit": "npx msw init public"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!**/src/*.{js,jsx,ts,tsx}",
      "!**/src/features/photoItem/*.{js,jsx,ts,tsx}",
      "!**/src/features/objectItem/*.{js,jsx,ts,tsx}",
      "!**/src/components/skyMap/*.{js,jsx,ts,tsx}"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!d3|d3-array|internmap|delaunator|robust-predicates)"
    ],
    "coverageReporters": [
      "lcov"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.176",
    "@types/node": "^12.20.36",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@types/react-router-dom": "^5.3.2",
    "msw": "^0.35.0",
    "sass": "^1.57.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
